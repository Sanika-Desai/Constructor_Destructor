#include<iostream>
using namespace std;

class conc
{
	public:
		conc(int a, int b)
		{
		//	a = 10;
		//	b = 8;
			cout<<"first constructor"<<endl;
			cout<<"a = "<<a<<endl<<"b = "<<b<<endl;
		}
		conc(float a, float b)
		{
		//	a = 8.8;
		//	b = 9.9;
		cout<<"second constructor"<<endl;
			cout<<"a = "<<a<<endl<<"b = "<<b<<endl;
		}
};

int main()
{
	float p =2.3;
	float q = 4.5;
	conc a(p,q);
	conc b(2,3);
}
